<br>
<button ID=WAV_Pause onclick="WAV_Send_CMD('P')">Pause</button>
<button ID=WAV_Continue onclick="WAV_Send_CMD('C')">Fortsetzen</button>
<button ID=WAV_Stop onclick="WAV_Send_CMD('S')">Stop</button>
<b>Volume (0-100%%):</b>
<input type=range class=slider ID=volume min=0 max=100 step=0.1 value=%WAV_Volume% oninput="document.getElementById('PWM0').value=volume.value;" onmouseup="WAV_Send_CMD('V '+volume.value)" ontouchend="WAV_Send_CMD('V '+volume.value)">
<input ID=PWM0 value=%WAV_Volume% size=2 readonly>
<button ID=WAV_Vol100 onclick="document.getElementById('volume').value=100;document.getElementById('PWM0').value=100;WAV_Send_CMD('V 100')">100%%</button>
<button ID=WAV_Vol0 onclick="document.getElementById('volume').value=0;document.getElementById('PWM0').value=0;WAV_Send_CMD('V 0')">0%%</button>
